From 63d2d3f422c2d808b699d14050fa10740f617369 Mon Sep 17 00:00:00 2001
From: Marty Jones <mj8263788@gmail.com>
Date: Tue, 20 Aug 2024 18:56:06 -0400
Subject: [PATCH] rtw88: manually backport rtl8812/21

---
 Kconfig.local                               | 18 +++++++
 drivers/net/wireless/realtek/rtw88/Kconfig  | 54 +++++++++++++++++++++
 drivers/net/wireless/realtek/rtw88/Makefile | 18 +++++++
 3 files changed, 90 insertions(+)

--- a/Kconfig.local
+++ b/Kconfig.local
@@ -1108,12 +1108,21 @@ config BACKPORTED_RTW88_8822B
 config BACKPORTED_RTW88_8822C
 	tristate
 	default RTW88_8822C
+config BACKPORTED_RTW88_8723X
+	tristate
+	default RTW88_8723X
+config BACKPORTED_RTW88_8703B
+	tristate
+	default RTW88_8703B
 config BACKPORTED_RTW88_8723D
 	tristate
 	default RTW88_8723D
 config BACKPORTED_RTW88_8821C
 	tristate
 	default RTW88_8821C
+config BACKPORTED_RTW88_8821A
+	tristate
+	default RTW88_8821A
 config BACKPORTED_RTW88_8822BE
 	tristate
 	default RTW88_8822BE
@@ -1138,6 +1147,9 @@ config BACKPORTED_RTW88_8723DE
 config BACKPORTED_RTW88_8723DS
 	tristate
 	default RTW88_8723DS
+config BACKPORTED_RTW88_8723CS
+	tristate
+	default RTW88_8723CS
 config BACKPORTED_RTW88_8723DU
 	tristate
 	default RTW88_8723DU
@@ -1150,6 +1162,12 @@ config BACKPORTED_RTW88_8821CS
 config BACKPORTED_RTW88_8821CU
 	tristate
 	default RTW88_8821CU
+config BACKPORTED_RTW88_8821AU
+	tristate
+	default RTW88_8821AU
+config BACKPORTED_RTW88_8812AU
+	tristate
+	default RTW88_8812AU
 config BACKPORTED_RTW88_DEBUG
 	tristate
 	default RTW88_DEBUG
--- a/drivers/net/wireless/realtek/rtw88/Kconfig
+++ b/drivers/net/wireless/realtek/rtw88/Kconfig
@@ -14,6 +14,7 @@ if RTW88
 config RTW88_CORE
 	tristate
 	depends on m
+	select BPAUTO_WANT_DEV_COREDUMP
 
 config RTW88_PCI
 	tristate
@@ -35,14 +36,28 @@ config RTW88_8822C
 	tristate
 	depends on m
 
+config RTW88_8723X
+	tristate
+	depends on m
+
+config RTW88_8703B
+	tristate
+	depends on m
+	select RTW88_8723X
+
 config RTW88_8723D
 	tristate
 	depends on m
+	select RTW88_8723X
 
 config RTW88_8821C
 	tristate
 	depends on m
 
+config RTW88_8821A
+	tristate
+	depends on m
+
 config RTW88_8822BE
 	tristate "Realtek 8822BE PCI wireless network adapter"
 	depends on m
@@ -139,6 +154,21 @@ config RTW88_8723DS
 
 	  802.11n SDIO wireless network adapter
 
+config RTW88_8723CS
+	tristate "Realtek 8723CS SDIO wireless network adapter"
+	depends on m
+	depends on MMC
+	select RTW88_CORE
+	select RTW88_SDIO
+	select RTW88_8703B
+	help
+	  Select this option to enable support for 8723CS chipset (EXPERIMENTAL)
+
+	  This module adds support for the 8723CS 802.11n SDIO
+	  wireless network adapter.
+
+	  If you choose to build a module, it'll be called rtw88_8723cs.
+
 config RTW88_8723DU
 	tristate "Realtek 8723DU USB wireless network adapter"
 	depends on m
@@ -187,6 +217,30 @@ config RTW88_8821CU
 
 	  802.11ac USB wireless network adapter
 
+config RTW88_8821AU
+	tristate "Realtek 8821AU USB wireless network adapter"
+	depends on m
+	depends on USB
+	select RTW88_CORE
+	select RTW88_USB
+	select RTW88_8821A
+	help
+	  Select this option will enable support for 8821AU chipset
+
+	  802.11ac USB wireless network adapter
+
+config RTW88_8812AU
+	tristate "Realtek 8812AU USB wireless network adapter"
+	depends on m
+	depends on USB
+	select RTW88_CORE
+	select RTW88_USB
+	select RTW88_8821A
+	help
+	  Select this option will enable support for 8812AU chipset
+
+	  802.11ac USB wireless network adapter
+
 config RTW88_DEBUG
 	bool "Realtek rtw88 debug support"
 	depends on RTW88_CORE
--- a/drivers/net/wireless/realtek/rtw88/Makefile
+++ b/drivers/net/wireless/realtek/rtw88/Makefile
@@ -44,6 +44,15 @@ rtw88_8822cs-objs		:= rtw8822cs.o
 obj-$(CPTCFG_RTW88_8822CU)	+= rtw88_8822cu.o
 rtw88_8822cu-objs		:= rtw8822cu.o
 
+obj-$(CPTCFG_RTW88_8723X)	+= rtw88_8723x.o
+rtw88_8723x-objs		:= rtw8723x.o
+
+obj-$(CPTCFG_RTW88_8703B)	+= rtw88_8703b.o
+rtw88_8703b-objs		:= rtw8703b.o rtw8703b_tables.o
+
+obj-$(CPTCFG_RTW88_8723CS)	+= rtw88_8723cs.o
+rtw88_8723cs-objs		:= rtw8723cs.o
+
 obj-$(CPTCFG_RTW88_8723D)	+= rtw88_8723d.o
 rtw88_8723d-objs		:= rtw8723d.o rtw8723d_table.o
 
@@ -68,6 +77,15 @@ rtw88_8821cs-objs		:= rtw8821cs.o
 obj-$(CPTCFG_RTW88_8821CU)	+= rtw88_8821cu.o
 rtw88_8821cu-objs		:= rtw8821cu.o
 
+obj-$(CPTCFG_RTW88_8821A)	+= rtw88_8821a.o
+rtw88_8821a-objs		:= rtw8821a.o rtw8821a_table.o rtw8812a_table.o
+
+obj-$(CPTCFG_RTW88_8821AU)	+= rtw88_8821au.o
+rtw88_8821au-objs		:= rtw8821au.o
+
+obj-$(CPTCFG_RTW88_8812AU)	+= rtw88_8812au.o
+rtw88_8812au-objs		:= rtw8812au.o
+
 obj-$(CPTCFG_RTW88_PCI)		+= rtw88_pci.o
 rtw88_pci-objs			:= pci.o
 
